<table id="example1" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>No</th>
            <th>Nama Coin</th>
            <th>Jumlah Coin</th>
            <th>Harga</th>
            <th>Aksi</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i=0; i<nominal.length; i++){ %>
            <tr>
                <td>
                    <%= i +1 %>
                </td>
                <td>
                    <%= nominal[i].coinName %>
                </td>
                <td>
                    <%= nominal[i].coinQuantity %>
                </td>
                <td>
                    <%= nominal[i].price %>
                </td>
                <td>
                    <form action="/nominal/delete/<%=nominal[i]._id%> ?_method=DELETE" method="POST">
                        <a href="/nominal/edit/<%=nominal[i]._id%>" class="btn btn-warning"><i
                                class="fas fa-pencil-alt"></i>Ubah</a>
                        <button class="btn btn-danger"><i class="fas fa-trash"></i>Hapus</button>
                    </form>
                </td>
            </tr>

            <%}%>

    </tbody>
</table>